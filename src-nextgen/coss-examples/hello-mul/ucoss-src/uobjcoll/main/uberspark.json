{

   	/* manifest header */
	"uberspark-manifest":{
		"manifest_node_types" : [ "uberspark-uobj" ],
		"uberspark_min_version" : "6.0.0",
		"uberspark_max_version" : "any"
	},


  	/* uobj declaration */
	"uberspark-uobj" : {
		"namespace" : "uberspark/uobjcoll/generic/hello-mul/main",
		"platform" : "generic",
		"arch" : "generic",
		"cpu" : "generic",

		/* sources */
		"sources" : {
			"h-files": [ ],
			"c-files": [ "main.c" ],
			"casm-files": [ "nullfunc.cS" ],
			"asm-files": [ ]
		},
	
		/* public methods */
		"publicmethods" : {
			"main" : [
				"uint32_t",
				"(uint32_t multiplicand, uint32_t multiplier)", 
				"2" 
			]
		},

		/* public methods of other uobjs that this uobj invokes 
			within the same collection
		*/
		"intrauobjcoll-callees" : {
		},
	

		/* public methods of other uobjs that this uobj invokes 
			across other collection(s)
		*/
		"interuobjcoll-callees": {
		},
	
		/* legacy CoSS functions/code that this uobj invokes */
		"legacy-callees": [
		]

	}

}



